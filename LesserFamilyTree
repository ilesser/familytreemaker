./familytreemaker.py LesserSterchele.txt -a "Tatarabuelo Sterchele"  > LesserSterchele.graph
./familytreemaker.py LesserSeghezza.txt  -a "Tatarabuelo Seghezza"   > LesserSeghezza.graph
dot LesserCazeaux.graph     -Tpng -o LesserCazeaux.png
dot LesserLesser.graph      -Tpng -o LesserLesser.png
dot LesserSterchele.graph   -Tpng -o LesserSterchele.png
dot LesserSeghezza.graph    -Tpng -o LesserSeghezza.png
